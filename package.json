{
    "name": "all-my-web",
    "version": "0.1.0",
    "description": "The single browser extention to manage all my web!",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/fankaidev/all-my-web.git"
    },
    "scripts": {
        "build": "vite build --config vite.config.chrome.ts",
        "build:chrome": "vite build --config vite.config.chrome.ts",
        "build:firefox": "vite build --config vite.config.firefox.ts",
        "dev": "nodemon --config nodemon.chrome.json",
        "dev:chrome": "nodemon --config nodemon.chrome.json",
        "dev:firefox": "nodemon --config nodemon.firefox.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "package": "cd dist_chrome && zip -r ../../all-my-web-chrome-$(cat manifest.json|jq -r .version).zip *"
    },
    "type": "module",
    "dependencies": {
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^7.1.1",
        "webextension-polyfill": "^0.12.0",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@crxjs/vite-plugin": "2.0.0-beta.30",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/chrome": "^0.0.278",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.1",
        "@types/webextension-polyfill": "^0.12.1",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/ui": "^2.1.8",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "fs-extra": "^11.2.0",
        "happy-dom": "^16.5.3",
        "nodemon": "^3.1.7",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.17",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "vite": "^6.0.7",
        "vite-tsconfig-paths": "^5.1.2",
        "vitest": "^2.1.8"
    }
}